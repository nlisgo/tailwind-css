{
  "name": "tailwind-css",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/css/style.css -o ./src/css/style-compiled.css --watch",
    "build": "npx tailwindcss -i ./src/css/style.css -o ./src/css/style-compiled.css --minify",
    "dev:elife": "npx tailwindcss -i ./src/css/elife.css -o ./src/css/elife-compiled.css --watch",
    "build:elife": "npx tailwindcss -i ./src/css/elife.css -o ./src/css/elife-compiled.css --minify",
    "build:all": "npm run build && npm run build:elife",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/language-server": "^0.14.29",
    "tailwindcss": "^4.1.17"
  }
}
